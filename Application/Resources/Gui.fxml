<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controler.GuiControler">
   <children>
      <AnchorPane layoutX="3.0" prefHeight="400.0" prefWidth="594.0">
         <children>
            <Label layoutX="15.0" layoutY="14.0" text="Zgłaszanie szkód turystycznych">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font></Label>
            <Button fx:id="pracownikId" layoutX="35.0" layoutY="104.0" mnemonicParsing="false" onAction="#handlePracownikAction" text="Pracownik" />
            <Button fx:id="klientId" layoutX="35.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleKlientAction" prefHeight="31.0" prefWidth="88.0" text="Klient" />
            <Label layoutX="35.0" layoutY="66.0" text="Zaloguj jako : ">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <ImageView fitHeight="224.0" fitWidth="395.0" layoutX="348.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
            <Button fx:id="wyjdzId" contentDisplay="TEXT_ONLY" layoutX="515.0" layoutY="355.0" mnemonicParsing="false" onAction="#handleWyjdzAction" text="Wyjdź" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
